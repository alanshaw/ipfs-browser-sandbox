{
  "name": "ipfs-browser-sandbox",
  "version": "0.2.1",
  "description": "An experimental p2p browser built on IPFS",
  "main": "lib/main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s build:*",
    "build:js": "babel src -d lib",
    "build:bundle": "webpack --config webpack.config.production.js",
    "build:copy": "run-p build:copy:*",
    "build:copy:css": "cp -R src/css lib",
    "build:copy:fonts": "cp -R src/fonts lib",
    "build:copy:images": "cp -R src/images lib",
    "build:copy:html": "cp src/index.html lib/index.html",
    "watch": "run-p watch:*",
    "watch:js": "npm run build:js -- --watch",
    "watch:bundle": "webpack --config webpack.config.development.js --watch",
    "clean": "rm -rf lib",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "IPFS",
    "browser",
    "p2p",
    "libp2p"
  ],
  "author": "Alan Shaw",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.5",
    "buffer": "^5.5.0",
    "cids": "^0.8.0",
    "file-type": "^14.1.4",
    "filesize": "^6.1.0",
    "ipfs": "^0.43.1",
    "ipfs-http-client": "^44.0.1",
    "ipfs-unixfs": "^1.0.0",
    "is-ipfs": "^1.0.3",
    "it-buffer": "^0.1.2",
    "it-pipe": "^1.1.0",
    "it-reader": "^2.1.0",
    "it-to-stream": "^0.1.1",
    "mime-types": "^2.1.26",
    "p-defer": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "electron": "^11.5.0",
    "electron-builder": "^22.4.1",
    "npm-run-all": "^4.1.5",
    "standard": "^14.3.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "build": {
    "appId": "io.ipfs.browser",
    "productName": "IPFS Browser Sandbox"
  }
}
