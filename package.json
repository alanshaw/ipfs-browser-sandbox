{
  "name": "planetarybrowser",
  "version": "0.0.0",
  "description": "An experimental p2p browser built on IPFS",
  "main": "lib/main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s build:*",
    "build:js": "babel src -d lib",
    "build:bundle": "webpack --config webpack.config.production.js",
    "build:copy": "run-p build:copy:*",
    "build:copy:css": "cp -R src/css lib",
    "build:copy:fonts": "cp -R src/fonts lib",
    "build:copy:html": "cp src/index.html lib/index.html",
    "watch": "run-p watch:*",
    "watch:js": "npm run build:js -- --watch",
    "watch:bundle": "webpack --config webpack.config.development.js --watch",
    "clean": "rm -rf lib"
  },
  "keywords": [
    "IPFS",
    "browser",
    "p2p",
    "libp2p"
  ],
  "author": "Alan Shaw",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.5",
    "ipfs": "^0.41.2",
    "it-pipe": "^1.1.0",
    "it-to-stream": "^0.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "electron": "^8.1.1",
    "npm-run-all": "^4.1.5",
    "standard": "^14.3.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
